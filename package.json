{
  "name": "youtube-video-quality-fixer",
  "description": "Chrome Extension for Fixing Highest Video Quality on YouTube.",
  "version": "0.0.11",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "@reduxjs/toolkit": "^1.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "redux-persist": "^6.0.0",
    "redux-persist-webextension-storage": "^1.0.2"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.16",
    "@fiahfy/eslint-config-react": "^0.0.21",
    "@fiahfy/prettier-config": "^0.0.3",
    "@fiahfy/tsconfig": "^0.0.2",
    "@types/chrome": "^0.0.241",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.25",
    "@types/redux-persist-webextension-storage": "^1.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "private": true,
  "productName": "Video Quality Fixer for YouTube",
  "scripts": {
